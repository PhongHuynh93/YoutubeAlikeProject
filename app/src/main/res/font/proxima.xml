<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto">
    <!--note Using the support library-->
    <!--<a href="https://developer.android.com/guide/topics/ui/look-and-feel/fonts-in-xml.html"></a>-->
    <font
        android:font="@font/proxima_nova_alt_regular_webfont"
        android:fontWeight="400"
        android:fontStyle="normal"
        app:font="@font/proxima_nova_alt_regular_webfont"
        app:fontWeight="400"
        app:fontStyle="normal"/>

    <font
        android:font="@font/proxima_nova_alt_bold_webfont"
        android:fontWeight="700"
        android:fontStyle="normal"
        app:font="@font/proxima_nova_alt_bold_webfont"
        app:fontWeight="700"
        app:fontStyle="normal"/>

    <font
        android:font="@font/proxima_nova_thin_webfont"
        android:fontWeight="400"
        android:fontStyle="italic"
        app:font="@font/proxima_nova_thin_webfont"
        app:fontWeight="400"
        app:fontStyle="italic"/>
</font-family>